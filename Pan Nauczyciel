def getGrade(grade):
    if grade >= 90:
        return "A"
    else:
        if grade <90 and grade >= 80:
            return "B"
        else:
            if grade <80 and grade >= 70:
                return "C"
            else:
                if grade <70 and grade >= 60:
                    return "D"
                else:
                    return "F"
        
grade = 79.9999
print("UczeÅ„ otrzymaÅ‚", getGrade(grade))
