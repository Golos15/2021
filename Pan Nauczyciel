def dajOcene(ocena):
    if ocena >= 90:
        return "5"
    else:
        if ocena <90 and ocena >= 80:
            return "4"
        else:
            if ocena <80 and ocena >= 70:
                return "3"
            else:
                if ocena <70 and ocena >= 60:
                    return "2"
                else:
                    return "1"
        
ocena = 79.9999
print("Uczeń otrzymał", dajOcene(ocena))
